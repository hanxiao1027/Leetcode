class Solution {
    public int countPrimes(int n) {
        boolean[] result = new boolean[n];
        int count = 0;
        for(int i = 2; i < n; i++){
            if(result[i] == false){
                count++;
                for(int j = 2; j * i < n; j++){
                    result[i * j] = true;
                }
            }
        }
        return count;
    }
}
