class Solution {
    public String intToRoman(int num) {
        List<Character> list= new ArrayList<>();
        while(num != 0){
            int curr = num % 26;
            if(curr == 0) curr = 26;
            char c = (char)('A' + curr - 1);
            list.add(c);
            num /= 26;
        }
        Collections.reverse(list);
        String result = "";
        for(Character c : list){
            result += c;
        }
        return result;
    }
}
