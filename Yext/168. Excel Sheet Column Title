class Solution {
    public String convertToTitle(int n) {
        List<Character> list = new ArrayList<>();
        while(n != 0){
            int curr = n % 26;
            if(curr == 0) curr = 26;
            char c = (char)('A' + curr - 1);
            list.add(c);
            n--;
            n /= 26;
            
        }
        Collections.reverse(list);
        String result = "";
        for(Character c : list){
            result += c;
        }
        return result;
        
    }
}
